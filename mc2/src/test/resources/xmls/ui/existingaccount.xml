<?xml version="1.0" encoding="UTF-8"?>
<suite name="Existing account" parallel="tests" thread-count="4">
    <test name="Sign in and change password" >
        <classes>
            <class name="endtoend.acceptance.signup.SignInOldUserTest" />
        </classes>
    </test>
    <test name="Test Phones">
        <classes>
            <class name="endtoend.acceptance.testphones.TestPhoneAddingTest" />
